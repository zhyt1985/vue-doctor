{
  "name": "vue-doctor",
  "version": "0.0.1",
  "description": "Diagnose and fix performance issues in your Vue app",
  "keywords": ["diagnostics", "linter", "nuxt", "performance", "vue"],
  "homepage": "https://github.com/zhyt1985/vue-doctor#readme",
  "bugs": {
    "url": "https://github.com/zhyt1985/vue-doctor/issues"
  },
  "license": "MIT",
  "author": "zhyt1985",
  "repository": {
    "type": "git",
    "url": "https://github.com/zhyt1985/vue-doctor.git",
    "directory": "packages/vue-doctor"
  },
  "bin": {
    "vue-doctor": "./dist/cli.js"
  },
  "files": ["dist"],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/cli.d.ts",
      "default": "./dist/cli.js"
    },
    "./api": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "rm -rf dist && tsup",
    "typecheck": "tsc --noEmit",
    "test": "pnpm build && vitest run"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "knip": "^5.83.1",
    "ora": "^7.0.1",
    "oxlint": "^1.47.0",
    "picocolors": "^1.1.1",
    "prompts": "^2.4.2",
    "typescript": ">=5.0.4 <7"
  },
  "devDependencies": {
    "@types/prompts": "^2.4.9",
    "tsup": "^8.0.0",
    "vitest": "^3.0.0"
  }
}
